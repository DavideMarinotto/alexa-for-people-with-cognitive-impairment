<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">Modify Alarm</h5>
            <button type="button" class="close modalClose" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form action="#" id="id_modifyAlarmForm" method="post">
                <div class="form-group">
                    <label for="message">Message:</label><br>
                    <input class="form-control" type="text" id="id_message" name="message" ><br>

                    <input class="form-control" type="hidden" id="id_cron" name="cron" value="#">
                    <input class="form-control" type="hidden" id="id_cronString" name="cronString" value="#" >
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary sendModify" id="id_modifyAlarm">Modify</button>
                    <button class="btn btn-danger modalClose"> Cancel </button>
                </div>
            </form>
        </div>

    </div>
</div>
